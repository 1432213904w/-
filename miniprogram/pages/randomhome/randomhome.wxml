<view class="page">
    <view class="wrap">
        <view>
            <text class="title">题库名称</text>
            <text class="car">{{use_title}}</text>
        </view>
        <view>
            <text class="title">题目数量</text>
            <text class="car">
                <text class="blue">{{50}}</text>题目</text>
        </view>
        <view>
            <text class="title">练习模式</text>
            <text class="car">随机练习</text>
        </view>
        <view>
            <text class="title">出题规则</text>
            <text class="car">从题库随机抽取50道题</text>
        </view>
 
        <view class="btn-area" bindtap="defaultGo">
        <text >开始练习</text>
        </view>
    </view>
</view>